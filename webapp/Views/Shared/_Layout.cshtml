    <script>
        var _settings = {
            siteUrl: '@System.Configuration.ConfigurationManager.AppSettings["siteUrl"]',
            isDev: '@System.Configuration.ConfigurationManager.AppSettings["isDev"]' == 'true',
            showProfilePicture: '@System.Configuration.ConfigurationManager.AppSettings["showProfilePicture"]' == 'true'
        };
        var _urlApi = '@System.Configuration.ConfigurationManager.AppSettings["urlApi"]';
    var _cookieName = '@System.Configuration.ConfigurationManager.AppSettings["cookieName"]';
    </script>
    @System.Web.Optimization.Scripts.Render("~/bundles/ember")
